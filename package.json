{
  "name": "vybzzz-platform",
  "version": "1.0.0",
  "description": "VyBzzZ - Plateforme de concerts en direct",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "check-env": "tsx scripts/check-env.ts",
    "check-env:prod": "tsx scripts/check-env.ts .env.production",
    "migrate": "./scripts/run-migrations.sh",
    "setup:stripe": "tsx scripts/setup-stripe.ts",
    "supabase:generate-types": "supabase gen types typescript --project-id YOUR_PROJECT_ID > types/supabase.ts",
    "test": "npm run test:ticket-purchase && npm run test:tip-payment && npm run test:webhooks && npm run test:payouts",
    "test:ticket-purchase": "tsx scripts/test-ticket-purchase.ts",
    "test:tip-payment": "tsx scripts/test-tip-payment.ts",
    "test:webhooks": "tsx scripts/test-webhook-processing.ts",
    "test:payouts": "tsx scripts/test-artist-payouts.ts",
    "test:all": "npm run test",
    "predev": "npm run check-env",
    "prebuild": "node -e \"if (process.env.RAILWAY_ENVIRONMENT || process.env.RAILWAY_SERVICE_NAME || process.env.CI || process.env.RAILWAY) { console.log('Skipping check-env in Railway/CI'); process.exit(0); } else { require('child_process').execSync('npm run check-env', {stdio: 'inherit'}); }\" || true"
  },
  "dependencies": {
    "@react-email/components": "^0.5.7",
    "@rork-ai/toolkit-sdk": "latest",
    "@sentry/nextjs": "^10.22.0",
    "@stripe/react-stripe-js": "^2.9.0",
    "@stripe/stripe-js": "^4.10.0",
    "@stripe/stripe-react-native": "0.50.3",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.45.0",
    "aws-sdk": "^2.1691.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "expo": "^54.0.32",
    "expo-localization": "~17.0.8",
    "expo-router": "^6.0.22",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.400.0",
    "next": "^14.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-email": "^4.3.2",
    "react-native": "^0.81.5",
    "react-native-web": "^0.21.2",
    "recharts": "^3.3.0",
    "resend": "^6.3.0",
    "stripe": "^16.0.0",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "^3.4.0",
    "tslib": "^2.8.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@expo/ngrok": "^4.1.0",
    "@types/node": "^20",
    "@types/react": "~19.1.10",
    "@types/react-dom": "~19.1.7",
    "autoprefixer": "^10.0.1",
    "dotenv": "^16.4.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.0",
    "postcss": "^8",
    "tsx": "^4.7.0",
    "typescript": "^5"
  },
  "private": true
}
